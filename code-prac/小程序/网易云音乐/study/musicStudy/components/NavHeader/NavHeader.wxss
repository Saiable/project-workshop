/* components/NavHeader.wxss */
.header {
    margin-bottom: 20rpx;
  }
.header .title {
    font-size: 32rpx;
    line-height: 40rpx;
    color: #666;
}
.header .item {
    height: 60rpx;
}
.header .desc {
    font-size: 30rpx;
    line-height: 60rpx;
}
.header .more {
    float: right;
    padding: 10rpx 20rpx;
    border: 1px solid #333;
    border-radius: 30rpx;
    font-size: 24rpx;
}